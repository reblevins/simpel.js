{
	"storage": {
		"simpleCMSDb": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"SimpelCMSPosts": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "simpleCMSDb",
					"attributes": [
						"Name",
						"Arn"
					],
					"attributeEnvMap": {
						"Name": "TABLE_NAME",
						"Arn": "TABLE_ARN"
					}
				}
			]
		}
	},
	"api": {
		"SimpleCMSAPI": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "SimpelCMSPosts",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"auth": {
		"cognito20ba519a": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation"
		}
	}
}